# Streamlit Cloud Secrets Configuration
# Copy these values to your Streamlit Cloud app's Secrets settings
# Settings > Secrets

# Required: Google Gemini API Key
GOOGLE_API_KEY = "your_google_api_key_here"

# LLM Configuration
LLM_PROVIDER = "google"
GEMINI_MODEL = "gemini-2.0-flash"

# Database (use in-memory for cloud)
DUCKDB_PATH = ":memory:"
